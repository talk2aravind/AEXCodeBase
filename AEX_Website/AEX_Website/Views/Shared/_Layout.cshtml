<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/css/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/css/carousel.css")" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <div class="navbar-wrapper">
        <div class="container">

            <div class="navbar navbar-inverse navbar-static-top" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">Adventure Explorers</a>
                    </div>
                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            @*<li><a href="#about">About</a></li>*@
                            <li>@*@Html.ActionLink("Fund Raising", "FundRaising", "Home")*@</li>
                            @*<li><a href="#contact">Fund Raising</a></li>*@
                            <li>@Html.ActionLink("Donate", "Donate", "Home")</li>
                            @*<li><a href="#contact">Donate</a></li>*@
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Core Programs <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Basic Climbing Class</a></li>
                                    <li><a href="#">Basic Rock Class</a></li>
                                    @*<li class="divider"></li>
                                    <li class="dropdown-header">Rainier Climbs</li>*@
                                    <li><a href="#">Intermediate Climbing Class</a></li>
                                    <li><a href="#">Canyoneering</a></li>
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="~/Views/Fundraisings/Index.cshtml" class="dropdown-toggle" data-toggle="dropdown">Fund Raising <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Adhoc Lesson</a></li>
                                    <li><a href="#">Miyar EBC Trek</a></li>
                                    <li class="divider"></li>
                                    <li class="dropdown-header">Rainier Climbs</li>
                                    <li><a href="#">With IMG</a></li>
                                    <li>@Html.ActionLink("With Kaf", "Fundraising", "Home")</li>
                                </ul>
                            </li>
                            <li>@Html.ActionLink("Store", "Store", "Home")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                            @*<li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">More <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">BCC</a></li>
                                    <li><a href="#">BRC</a></li>
                                    <li><a href="#">ICC</a></li>
                                    <li class="divider"></li>
                                    <li class="dropdown-header">Nav header</li>
                                    <li><a href="#">Separated link</a></li>
                                    <li><a href="#">One more separated link</a></li>
                                </ul>
                            </li>*@
                            @Html.Partial("_LoginPartial")
                         </ul>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            @*<li data-target="#myCarousel" data-slide-to="3"></li>*@
        </ol>
        <div class="carousel-inner">
            <div class="item active">
                @*<img src="http://placehold.it/900x500/777777&text=First+slide" alt="First slide">*@
                <img src="~/Images/IMG_9114.jpg" alt="First slide" width="3264" height="2448" />
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Basic Climbing Class</h1>
                        <p>Our basic climbing class offers excellent learning ground for beginners who wants to break out of their hiking shell and get into more technical climbing and basic level climbing. </p>
                        <p><a class="btn btn-lg btn-primary" href="https://www.adventure-explorers.com/bcc-2015-application.html" role="button">Sign up today</a></p>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="~/Images/IMG_9117.jpg" alt="second slide" width="3264" height="2448">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Basic Rock Climbing Class</h1>
                        <p>Our Basic Rock Class offers a lot of top roped climbs. Plenty of sports climbs. Introduction into Trad Climbing. Covers Multi Pitch climbing.</p>
                        <p><a class="btn btn-lg btn-primary" href="#" role="button">Learn more</a></p>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="~/Images/IMG_9121.jpg" alt="Third slide" width="3264" height="2448">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Intermediate Climbing Class.</h1>
                        <p>The Goals are split into several categories  Leading Rock Leading Sports Rock Leading Trad Rock Leading Multipitch and swinging Leads Leading Snow, Organize Basic Climbs and Become a Leader.</p>
                        <p><a class="btn btn-lg btn-primary" href="#" role="button">Browse gallery</a></p>
                    </div>
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
    </div>
    @RenderBody()

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="@Url.Content("~/js/bootstrap.js")"></script>
    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>